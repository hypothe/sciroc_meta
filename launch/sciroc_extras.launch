<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <!-- usb_cam node publishing img from development pc connected camera -->
  <node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
    <param name="video_device" value="/dev/video0" />
    <param name="image_width" value="640" />
    <param name="image_height" value="480" />
    <param name="pixel_format" value="yuyv" />
    <param name="camera_frame_id" value="xtion_optical_frame" />
    <param name="io_method" value="mmap"/>
    <param name="camera_info_url" value="file://$(find tiago_998_gazebo)/config/camera_calibration.yaml" />
  </node>
  <!-- audio_capture launch file to get data from development pc connected microphone -->
  <include file="$(find audio_capture)/launch/capture.launch"></include>


  <!--node name="sciroc_ep1_object_manager_node" pkg="sciroc_ep1_object_manager" type="sciroc_ep1_object_manager_node.py" output="screen"/-->
  
</launch>
